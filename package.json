{
  "name": "materiallab-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "design-system": "cross-env DESIGN_SYSTEM_MODE=true vite --port 2000",
    "build": "vite build",
    "build-with-types": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "playwright test",
    "test:visual": "playwright test tests/visual",
    "test:e2e": "playwright test tests/e2e",
    "test:design-system": "playwright test tests/design-system",
    "test:accessibility": "playwright test tests/accessibility",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "validate:tokens": "node scripts/validate-tokens.js",
    "validate:atomic": "node scripts/validate-atomic-structure.js",
    "validate:themes": "node scripts/validate-theme-support.js",
    "validate:performance": "bundlesize",
    "validate:all": "npm run validate:tokens && npm run validate:atomic && npm run validate:themes && npm run validate:performance",
    "claude:quick-check": "tsx scripts/quick-check.ts",
    "claude:full-validation": "tsx scripts/run-validation.ts full",
    "claude:correction-loop": "tsx scripts/run-validation.ts loop",
    "claude:material-design": "tsx scripts/run-validation.ts quick --component",
    "claude:brand-check": "tsx scripts/validation-pipeline.ts",
    "claude:context-load": "tsx scripts/context-loader.ts",
    "claude:test": "tsx scripts/test-orchestration.ts",
    "claude:quality-gates": "tsx scripts/quality-gates.ts",
    "atomic:registry": "node scripts/generate-component-registry.js",
    "atomic:migrate": "node scripts/migrate-component.js",
    "atomic:create": "node scripts/create-component.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.6",
    "@fontsource-variable/sora": "^5.2.6",
    "@fontsource-variable/space-grotesk": "^5.2.8",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "tsx": "^4.7.0",
    "commander": "^11.1.0",
    "vite": "^7.1.2"
  }
}
